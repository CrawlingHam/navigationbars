        "@radix-ui/react-tooltip": "^1.1.8",
        "@reduxjs/toolkit": "^2.6.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cross-env": "^7.0.3",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-icons": "^5.5.0",
        "react-redux": "^9.2.0",
        "tailwind-merge": "^3.1.0"
