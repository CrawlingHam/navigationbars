{
    "name": "navigationbar",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "packageManager": "pnpm@10.6.4",
    "scripts": {
        "build:only": "tsc -b && vite build",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
