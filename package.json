{
    "name": "navigationbar-vue",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "packageManager": "pnpm@9.15.4",
    "scripts": {
        "build": "pnpm run format && pnpm run lint && pnpm run build:only",
        "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
